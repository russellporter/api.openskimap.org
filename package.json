{
  "name": "api.openskimap.org",
  "version": "1.0.0",
  "description": "REST API for OpenSkiMap.org frontend",
  "main": "src/index.ts",
  "dependencies": {
    "express": "^4.22.1",
    "JSONStream": "^1.3.5",
    "openskidata-format": "^7.0.0",
    "pg": "^8.12.0",
    "stream-to-promise": "^3",
    "tslib": "^2",
    "uuid": "^10"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/pg": "^8.11.0",
    "@types/geojson": "^7946.0.14",
    "@types/stream-to-promise": "^2.2.0",
    "@types/uuid": "^10",
    "prettier": "^3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "scripts": {
    "import-data": "[ \"$NODE_ENV\" = \"production\" ] && node dist/scripts/import_data.js || node -r ts-node/register src/scripts/import_data.ts",
    "drop-database": "[ \"$NODE_ENV\" = \"production\" ] && node dist/scripts/drop_database.js || node -r ts-node/register src/scripts/drop_database.ts",
    "build": "tsc -p .",
    "watch": "tsc -w -p .",
    "debug": "nodemon --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/index.js",
    "docker-debug": "docker-compose up",
    "start": "node ./dist/index.js",
    "check-types": "tsc"
  },
  "author": "Russell Porter",
  "license": "ISC"
}
