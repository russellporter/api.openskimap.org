{
  "name": "api.openskimap.org",
  "version": "1.0.0",
  "description": "REST API for OpenSkiMap.org frontend",
  "main": "src/index.ts",
  "dependencies": {
    "@types/express": "^4.17.21",
    "arangojs": "^9",
    "express": "^4.20.0",
    "JSONStream": "^1.3.5",
    "openskidata-format": "^0.14.0",
    "stream-to-promise": "^3",
    "tslib": "^2",
    "uuid": "^10"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.14",
    "@types/stream-to-promise": "^2.2.0",
    "@types/uuid": "^10",
    "prettier": "^3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "scripts": {
    "import-data": "node -r ts-node/register src/scripts/import_data.ts",
    "drop-database": "node -r ts-node/register src/scripts/drop_database.ts",
