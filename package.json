{
  "name": "api.openskimap.org",
  "version": "1.0.0",
  "description": "REST API for OpenSkiMap.org frontend",
  "main": "src/index.ts",
  "dependencies": {
    "@types/express": "^4.16.1",
    "arangojs": "^6.10.0",
    "express": "^4.16.4",
    "JSONStream": "^1.3.5",
    "openskidata-format": "^0.12.0",
    "stream-to-promise": "^2.2.0",
    "tslib": "^1.14.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.7",
    "@types/stream-to-promise": "^2.2.0",
    "@types/uuid": "^3.4.6",
    "prettier": "^2.0.5",
    "ts-node": "^8.2.0",
    "typescript": "4"
  },
  "scripts": {
    "import-data": "node -r ts-node/register src/scripts/import_data.ts",
    "drop-database": "node -r ts-node/register src/scripts/drop_database.ts",
